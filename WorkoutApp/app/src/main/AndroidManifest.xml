<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sebnem.workoutapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AllWorkoutsPage"
            android:label="@string/action_all_workouts"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AllExercisesPage"
            android:label="@string/action_all_exercises"
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".AddExerciseActivity"
            android:label="Create Exercise"
            android:parentActivityName=".AllExercisesPage" />

        <activity
            android:name=".WorkoutStart"
            android:label=""
            android:parentActivityName=".MainActivity" />

        <activity
            android:name=".WorkoutDetailsPage"
            android:label=""
            android:parentActivityName=".AllWorkoutsPage" />
        <activity
            android:name=".ExerciseDetailsPage"
            android:label=""
            android:parentActivityName=".AllExercisesPage" />
        <activity
            android:name=".ProfileActivity"
            android:label="@string/action_profile"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/action_settings"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AddWorkoutActivity"
            android:label="Create Workout"
            android:parentActivityName=".AllWorkoutsPage" />
        <activity
            android:name=".EditWorkoutActivity"
            android:label="Edit Workout"
            android:parentActivityName=".AllWorkoutsPage"/>
    </application>

</manifest>